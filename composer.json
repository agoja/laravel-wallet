{
  "name": "bavix/laravel-wallet",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^7.1|^8.0",
    "illuminate/database": "^5.5|^6.0",
    "nesbot/carbon": "^1.20|^2.0",
    "doctrine/dbal": "^2.8",
    "ramsey/uuid": "^3.0"
  },
  "suggest": {
    "bavix/laravel-wallet-swap": "Addition to the laravel-wallet library for quick setting of exchange rates"
  },
  "autoload": {
    "psr-4": {
      "Bavix\\Wallet\\": "src/"
    }
  },
  "extra": {
    "laravel": {
